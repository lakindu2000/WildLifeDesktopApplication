<AnchorPane fx:id="context" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.developersstack.wildlife.controller.SignupFormController">
   <children>
      <Separator layoutX="216.0" layoutY="51.0" prefHeight="0.0" prefWidth="378.0" />
      <TextField fx:id="txtFirstName" layoutX="216.0" layoutY="74.0" prefHeight="25.0" prefWidth="145.0" promptText="First Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="txtLastName" layoutX="379.0" layoutY="74.0" prefHeight="27.0" prefWidth="152.0" promptText="Last Name">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="txtEmail" layoutX="216.0" layoutY="111.0" prefHeight="25.0" prefWidth="318.0" promptText="Email">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="216.0" layoutY="145.0" prefHeight="25.0" prefWidth="318.0" promptText="Password">
